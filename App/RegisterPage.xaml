<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EnterpriseMarketplace.RegisterPage"
             Title="Register">
    <StackLayout Padding="10">
        <Entry x:Name="UsernameEntry" Placeholder="Username" />
        <Entry x:Name="PasswordEntry" Placeholder="Password" IsPassword="True" />
        <Entry x:Name="EmailEntry" Placeholder="Email" />
        <Entry x:Name="AgeEntry" Placeholder="Age" Keyboard="Numeric" />
        <Entry x:Name="LocationEntry" Placeholder="Location" />
        <Entry x:Name="PhoneEntry" Placeholder="Phone" Keyboard="Telephone" />
        <Button Text="Register" Clicked="OnRegisterButtonClicked" />
    </StackLayout>
</ContentPage>